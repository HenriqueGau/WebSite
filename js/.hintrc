{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/css": [
      "off",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 128",
    "not chrome <= 129",
    "not edge <= 128",
    "not firefox <= 130",
    "not safari <= 18",
    "not samsung <= 25"
  ]
}